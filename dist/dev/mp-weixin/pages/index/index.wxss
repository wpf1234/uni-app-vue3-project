.card {
  /* Rectangle 1 */
  position: absolute;
  width: 390px;
  height: 844px;
  left: 0px;
  top: 0px;
  background: linear-gradient(180deg, #d94646 48.44%, rgba(243, 50, 50, 0) 100%);
}
